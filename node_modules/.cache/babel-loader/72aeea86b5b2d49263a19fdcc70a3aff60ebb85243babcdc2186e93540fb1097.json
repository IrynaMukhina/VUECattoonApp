{"ast":null,"code":"import { computed } from 'vue';\nimport { useStore } from 'vuex';\nexport default {\n  props: ['character'],\n  setup(props) {\n    const store = useStore();\n    const isFavorite = computed(() => store.getters.isFavorite(props.character.id));\n    const toggleFavorite = () => {\n      if (isFavorite.value) {\n        store.commit('removeFavorite', props.character.id);\n      } else {\n        store.commit('addFavorite', props.character);\n      }\n    };\n    return {\n      isFavorite,\n      toggleFavorite\n    };\n  }\n};","map":{"version":3,"names":["computed","useStore","props","setup","store","isFavorite","getters","character","id","toggleFavorite","value","commit"],"sources":["/Users/imukhina/Desktop/VueCartoonProject/VUECattoonApp/src/components/CharacterCard.vue"],"sourcesContent":["<template>\n  <div class=\"character-card\">\n    <img :src=\"character.image\" :alt=\"character.name\" />\n    <h3>{{ character.name }}</h3>\n    <p>Species: {{ character.species }}</p>\n    <p>Status: {{ character.status }}</p>\n    <button @click=\"toggleFavorite\">\n      {{ isFavorite ? 'Remove from Favorites' : 'Add to Favorites' }}\n    </button>\n  </div>\n</template>\n\n<script>\nimport { computed } from 'vue';\nimport { useStore } from 'vuex';\n\nexport default {\n  props: ['character'],\n  setup(props) {\n    const store = useStore();\n    const isFavorite = computed(() => store.getters.isFavorite(props.character.id));\n\n    const toggleFavorite = () => {\n      if (isFavorite.value) {\n        store.commit('removeFavorite', props.character.id);\n      } else {\n        store.commit('addFavorite', props.character);\n      }\n    };\n\n    return { isFavorite, toggleFavorite };\n  },\n};\n</script>\n"],"mappings":"AAaA,SAASA,QAAO,QAAS,KAAK;AAC9B,SAASC,QAAO,QAAS,MAAM;AAE/B,eAAe;EACbC,KAAK,EAAE,CAAC,WAAW,CAAC;EACpBC,KAAKA,CAACD,KAAK,EAAE;IACX,MAAME,KAAI,GAAIH,QAAQ,CAAC,CAAC;IACxB,MAAMI,UAAS,GAAIL,QAAQ,CAAC,MAAMI,KAAK,CAACE,OAAO,CAACD,UAAU,CAACH,KAAK